    "lodash-es": "^4.17.21",
    "pinia": "^2.0.13",
    "qs": "^6.10.3",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.3.0",
    "autoprefixer": "^10.4.4",
    "less": "^4.1.2",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.12",
    "rollup": "^2.70.2",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.7.0",
    "unplugin-vue-components": "^0.19.2",
